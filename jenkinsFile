@Library('isfpLib') _

def config=[:]

pipeline {
    tools{
    maven 'maven 3.3.3'
    }
    agent any

    stages {
        stage('prepare build and deployment stage') {
            steps {
                script{
                   config=projectConfig(project)
                    
                }
            }
        }
        stage('Build Project'){
            steps{
                node(config.buildNode){
                    script{
                    if(projectSVN()){
                        buildProject()
                        }
                    }
                }
            }
        }
        stage('Deploy Patch on Project '){
            steps{
                echo 'deploy'
            }
        }
    }
}